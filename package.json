{
    "name": "yyabuilder",
    "version": "1.1.6",
    "private": false,
    "description": "Settings and tools for building typescript projects",
    "type": "commonjs",
    "main": "bin/yb.js",
    "bin": {
        "yb": "bin/yb.js",
        "ybuild": "bin/yb.js",
        "yyabuilder": "bin/yb.js"
    },
    "files": [
        ".gitignore",
        "package.json",
        "LICENSE",
        "UNLICENSE",
        "bin/**/*.*",
        "lib/**/*.*",
        "src/**/*.*",
        "version.cjs",
        "readme.md"
    ],
    "scripts": {
        "start": "node bin/yb.js",
        "test": "jest",
        "build": "echo No build needed",
        "republish": "npm run build && npx version-select && npx genversion --semi version.cjs && npm publish && npm uninstall -g yyabuilder && npm i -g yyabuilder --legacy-peer-deps && yb --version"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yuyaryshev/yyabuilder.git"
    },
    "keywords": [
        "yyabuilder",
        "babel",
        "typescript",
        "bundler"
    ],
    "author": "Yuri Yaryshev",
    "license": "UNLICENSED",
    "publishConfig": {
        "registry": "http://registry.npmjs.org"
    },
    "bugs": {
        "url": "https://github.com/yuyaryshev/yyabuilder/issues"
    },
    "homepage": "https://github.com/yuyaryshev/yyabuilder#readme",
