{
    "name": "ybuilder",
    "version": "1.0.11",
    "private": false,
    "description": "Settings and tools for building typescript projects",
    "type": "commonjs",
    "main": "bin/yb.js",
    "bin": {
        "yb": "bin/yb.js",
        "ybuild": "bin/yb.js",
        "ybuilder": "bin/yb.js"
    },
    "files": [
        ".gitignore",
        "package.json",
        "LICENSE",
        "UNLICENSE",
        "bin/**/*.*",
        "lib/**/*.*",
        "src/**/*.*",
        "version.cjs",
        "readme.md"
    ],
    "scripts": {
        "start": "node bin/yb.js",
        "test": "jest",
        "build": "echo No build needed",
        "republish": "npm run build && npx version-select && npx genversion --semi version.cjs && npm publish && npm uninstall -g ybuilder && npm i -g ybuilder --legacy-peer-deps && yb --version"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yuyaryshev/ybuilder.git"
    },
    "keywords": [
        "ybuilder",
        "babel",
        "typescript",
        "bundler"
    ],
    "author": "Yuri Yaryshev",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/yuyaryshev/ybuilder/issues"
    },
    "homepage": "https://github.com/yuyaryshev/ybuilder#readme",
    "plainDependencies": true,
    "dependencies": {
        "@babel/cli": "7.12.8",
        "@babel/core": "7.12.9",
        "@babel/node": "^7.12.6",
